{
  "cache": false,
  "check-coverage": true,
  "lines": 50,
  "statements": 50,
  "functions": 50,
  "branches": 50,
  "extension": [
    ".ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/app.ts",
    "src/**/index.ts",
    "src/**/health-monitor.ts"
  ],
  "sourceMap": true,
  "reporter": [
    "html",
    "text",
    "text-summary"
  ],
  "all": true,
  "instrument": true
}